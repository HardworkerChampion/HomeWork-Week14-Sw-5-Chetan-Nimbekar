<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="testsuite" verbose="1" >
    <groups>
        <run>
            <include name="sanity"></include>
        </run>
    </groups>
    <listeners>
        <listener class-name="com.nopcommerce.demo.customlisteners.CustomListeners"/>

    </listeners>
    <test name="Login Test"   >
        <classes>
           <class name="com.nopcommerce.demo.testsuite.ComputersPageTest"></class>
           <class name="com.nopcommerce.demo.testsuite.LoginPageTest"></class>
           <class name="com.nopcommerce.demo.testsuite.RegisterPageTest"></class>
        </classes>

    </test>
</suite>
